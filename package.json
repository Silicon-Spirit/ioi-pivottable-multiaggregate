{
	"name": "vue3-pivottable",
	"version": "1.0.0",
	"description": "A Vue 3 pivot table component for Frappe Framework",
	"type": "module",
	"main": "./dist/vue3-pivottable.umd.js",
	"module": "./dist/vue3-pivottable.es.js",
	"exports": {
		".": {
			"import": "./dist/vue3-pivottable.es.js",
			"require": "./dist/vue3-pivottable.cjs.js"
		},
		"./frappe": {
			"import": "./dist/vue3-pivottable-frappe.es.js",
			"require": "./dist/vue3-pivottable-frappe.umd.js"
		},
		"./style": "./dist/style.css"
	},
	"files": [
		"dist",
		"src",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"dev": "vite",
		"build": "vite build && vite build --config vite.frappe.config.js",
		"preview": "vite preview",
		"lint": "eslint src --ext .js"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git://github.com/Silicon-Spirit/vue3-pivottable.git"
	},
	"keywords": [
		"vue3",
		"pivot-table",
		"frappe",
		"data-analysis",
		"pivot",
		"table",
		"chart",
		"visualization"
	],
	"peerDependencies": {
		"vue": "^3.0.0"
	},
	"dependencies": {
		"vuedraggable": "^4.1.0",
		"xlsx": "^0.18.5"
	},
	"devDependencies": {
		"@vitejs/plugin-vue": "^5.0.0",
		"vite": "^5.0.0"
	}
}
